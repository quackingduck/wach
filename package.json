{
  "name": "wach",
  "description": "Run a command when a file add/update event occurs",
  "version": "1.0.0",
  "main": "./lib/wach",
  "bin": {
    "wach": "./bin/wach",
    "wachs": "./bin/wachs"
  },
  "engines": {
    "node": ">=0.4.0 <5.0.0"
  },
  "dependencies": {
    "minimatch": ">=0.0.5",
    "wachdir": "~0.1.0"
  },
  "devDependencies": {
    "coffee-script": "~1.6.3",
    "mocha": "0.14.1"
  },
  "os": [
    "darwin"
  ],
  "author": "Myles Byrne",
  "repository": {
    "type": "git",
    "url": "git://github.com/quackingduck/wach.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/quackingduck/wach/raw/master/LICENSE"
    }
  ],
  "keywords": [
    "watch",
    "filesystem",
    "fsevents",
    "monitor",
    "supervise",
    "observe"
  ],
  "scripts": {
    "build": "coffee -co lib src"
  }
}
